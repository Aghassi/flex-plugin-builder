{
  "name": "@twilio-labs/plugin-flex",
  "version": "4.4.0-beta.0",
  "description": "Create, develop and deploy Flex plugins using the Twilio CLI",
  "keywords": [
    "twilio",
    "flex",
    "twilio flex",
    "flex plugins",
    "flex plugins cli",
    "flex plugins api",
    "oclif-plugin"
  ],
  "homepage": "https://github.com/twilio/flex-plugin-builder",
  "bugs": {
    "url": "https://github.com/twilio/flex-plugin-builder/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twilio/flex-plugin-builder.git"
  },
  "license": "MIT",
  "author": "Twilio @twilio",
  "files": [
    "/oclif.manifest.json",
    "/dist",
    "/package-lock.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "../../node_modules/.bin/tsc",
    "clean": "rm -rf dist",
    "lint": "eslint --ext ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prepack": "./bin/prepack",
    "postpack": "rm -f oclif.manifest.json",
    "prepublish": "npm run build",
    "replace-doc": "cp docs/README.md README.md && oclif-dev readme",
    "test": "cd ../.. && jest packages/flex-plugin-webpack --color",
    "test:watch": "cd ../.. && jest packages/flex-plugin-webpack --watch --color"
  },
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/errors": "^1.3.4",
    "@oclif/parser": "^3.8.5",
    "@twilio/cli-core": "^5.15.1",
    "create-flex-plugin": "^4.4.0-beta.0",
    "dayjs": "^1.10.2",
    "flex-dev-utils": "^4.4.0-beta.0",
    "flex-plugin-scripts": "^4.4.0-beta.0",
    "flex-plugins-api-client": "^0.22.2",
    "flex-plugins-api-toolkit": "^0.22.2",
    "lodash.startcase": "^4.4.0",
    "mkdirp": "^1.0.4",
    "package-json": "^6.5.0",
    "phin": "^3.5.1",
    "rimraf": "^3.0.2",
    "semver": "^7.3.4",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@types/lodash.startcase": "^4.4.6",
    "@types/mkdirp": "^1.0.1",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.4",
    "@types/tmp": "^0.2.0",
    "@types/yargs": "^15.0.12",
    "globby": "^11.0.2",
    "tmp": "^0.2.1",
    "twilio": "^3.54.2"
  },
  "engines": {
    "node": ">=10.12.0"
  },
  "oclif": {
    "name": "flex",
    "commands": "./dist/commands",
    "bin": "twilio",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "flex": {
        "description": "Create, develop and deploy Flex plugins using the Twilio CLI"
      }
    }
  }
}
